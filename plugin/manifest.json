{
    "manifest_version": 3,
    "name": "check-pvp-parser",
    "version": "0.0.1",
    "description": "check-pvp parser",
    "action": {
        "default_popup": "src/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://check-pvp.fr/*"
            ],
            "js": [
                "src/content.ts"
            ]
        }
    ]
}
